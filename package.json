{
	"name": "my-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"start": "yarn prepare && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "yarn format && yarn lint &&  yarn check &&  yarn optimize",
		"optimize": "vite optimize --force",
		"start-build": "yarn prepare && yarn build && yarn preview"
	},
	"devDependencies": {
		"@playwright/test": "^1.46.1",
		"@skeletonlabs/skeleton": "^1.2.5",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/kit": "1.15.10",
		"@tailwindcss/forms": "^0.5.7",
		"@types/cookie": "^0.5.4",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.20",
		"cssnano": "^6.1.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.41",
		"postcss-import": "^15.1.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.77.8",
		"svelte": "^3.59.2",
		"svelte-check": "^3.8.5",
		"svelte-preprocess": "^5.1.4",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.6.3",
		"typescript": "^4.9.5",
		"uuid": "^9.0.1",
		"vite": "^4.5.3",
		"vite-imagetools": "^5.1.2",
		"vitest": "^2.0.5"
	},
	"type": "module",
	"volta": {
		"node": "20.17.0",
		"yarn": "4.4.0"
	},
	"packageManager": "yarn@4.4.0"
}
