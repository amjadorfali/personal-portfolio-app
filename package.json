{
	"name": "my-app",
	"version": "0.0.1",
	"scripts": {
		"start": "yarn prepare && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "yarn format && yarn lint &&  yarn check &&  yarn optimize",
		"optimize": "vite optimize --force",
		"start-build": "yarn prepare && yarn build && yarn preview"
	},
	"devDependencies": {
		"@playwright/test": "^1.32.3",
		"@sendgrid/mail": "^7.7.0",
		"@skeletonlabs/skeleton": "^1.2.0",
		"@sveltejs/adapter-netlify": "^2.0.6",
		"@sveltejs/kit": "1.15.7",
		"@tailwindcss/forms": "^0.5.3",
		"@types/cookie": "^0.5.1",
		"@types/gtag.js": "^0.0.12",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gsap": "^3.11.5",
		"postcss": "^8.4.23",
		"postcss-import": "^15.1.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.3.1",
		"vite-imagetools": "^4.0.19",
		"vitest": "^0.26.3"
	},
	"type": "module",
	"volta": {
		"node": "18.14.1",
		"yarn": "3.4.1"
	}
}
